#!/bin/sh
# prerm script for zulu-kernel-image
#
# see: dh_installdeb(1)

set -e

export INITRD=Yes
if [ -d "/etc/kernel/prerm.d" ]; then
    run-parts -v --report --exit-on-error --arg=#KERNELRELEASE# --arg=/boot/vmlinux-#KERNELRELEASE# /etc/kernel/prerm.d
fi


# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
